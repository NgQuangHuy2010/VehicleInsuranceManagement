@model Project3.Models.VehicleWarranty

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/System/Views/Layout/LayoutAdmin.cshtml";
}

<h1>Edit</h1>

<h4>VehicleWarranty</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="WarrantyId" />
            <div class="form-group">
                <label asp-for="WarrantyType" class="control-label"></label>
                <input asp-for="WarrantyType" class="form-control" />
                <span asp-validation-for="WarrantyType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="WarrantyDuration" class="control-label"></label>
                <input asp-for="WarrantyDuration" class="form-control" />
                <span asp-validation-for="WarrantyDuration" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="WarrantyDetails" class="control-label"></label>
                <input asp-for="WarrantyDetails" class="form-control" />
                <span asp-validation-for="WarrantyDetails" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
